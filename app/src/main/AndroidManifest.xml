<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="m.incrementrestservice.poulouloureminder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RdvActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".NewRdvActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".NewNoteActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NotesActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MessageActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="pouloulou reminder"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".fgtPassword" />
        <activity android:name=".login" />
        <activity android:name=".sign_up" />
        <activity android:name=".Chargement">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".notifications.MyFirebaseMessaging"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name=".notifications.MyfirebaseIdService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>